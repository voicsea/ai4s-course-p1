{
    "model_name": "adabins",
    "model_kwargs": {
        "n_bins": 128,
        "bins_min": 0.0,
        "bins_max": 1.0,
        "backbone": "resnet50",
        "pretrained": true,
        "dropout": 0.1,
        "use_aspp": true,
        "use_fpn": true,
        "use_depth_refinement": true,
        "use_feature_fusion": true,
        "bin_regularization_weight": 0.01,
        "edge_aware_weight": 0.1
    },
    "num_epochs": 60,
    "batch_size": 8,
    "learning_rate": 1e-4,
    "weight_decay": 1e-4,
    "optimizer_name": "adamw",
    "scheduler_name": "reduce_on_plateau",
    "scheduler_kwargs": {
        "mode": "min",
        "factor": 0.5,
        "patience": 5,
        "verbose": true
    },
    "pixel_loss": "l1",
    "gradient_loss": "gradient",
    "scale_invariant_loss": true,
    "bin_regularization": true,
    "pixel_weight": 1.0,
    "gradient_weight": 0.5,
    "scale_invariant_weight": 0.5,
    "data_root": "../../ImageToDEM/singleRGBNormalization",
    "test_root": "../../ImageToDEM/singleRGBNormalizationTest",
    "train_val_split": 0.1,
    "num_workers": 0,
    "pin_memory": false,
    "input_size": 512,
    "random_crop": true,
    "flip_augmentation": true,
    "brightness_jitter": 0.2,
    "contrast_jitter": 0.2,
    "checkpoint_dir": "./checkpoints",
    "save_frequency": 5,
    "device": "cuda"
}
