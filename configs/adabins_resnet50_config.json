{
    "model_name": "adabins",
    "model_kwargs": {
        "n_bins": 128,
        "bins_min": 0.0,
        "bins_max": 1.0,
        "backbone": "resnet50",
        "pretrained": false,
        "dropout": 0.0
    },
    "num_epochs": 60,
    "batch_size": 8,
    "learning_rate": 1e-4,
    "weight_decay": 1e-4,
    "optimizer_name": "adamw",
    "scheduler_name": "reduce_on_plateau",
    "scheduler_kwargs": {
        "mode": "min",
        "factor": 0.5,
        "patience": 5,
        "verbose": true
    },
    "pixel_loss": "l1",
    "gradient_loss": "gradient",
    "pixel_weight": 1.0,
    "gradient_weight": 0.5,
    "data_root": "../../ImageToDEM/singleRGBNormalization",
    "test_root": "../../ImageToDEM/singleRGBNormalizationTest",
    "train_val_split": 0.1,
    "num_workers": 4,
    "pin_memory": true,
    "input_size": 512,
    "checkpoint_dir": "./checkpoints",
    "save_frequency": 5,
    "device": "cuda"
}
